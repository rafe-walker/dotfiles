! Set font for urxvt
URxvt.font: xft:Hack-Regular:size=15

! Disable scrollbar in urxvt
URxvt.scrollBar: false

! Create transparent background
URxvt.depth: 32
URxvt.background: [85]#2f343f

! special
*.foreground:   #a8a19f
*.background:   #1b1918
*.cursorColor:  #a8a19f

! black
*.color0:       #1b1918
*.color8:       #766e6b

! red
*.color1:       #f22c40
*.color9:       #f22c40

! green
*.color2:       #5ab738
*.color10:      #5ab738

! yellow
*.color3:       #d5911a
*.color11:      #d5911a

! blue
*.color4:       #3182bd
*.color12:      #3182bd

! magenta
*.color5:       #6666ea
*.color13:      #6666ea

! cyan
*.color6:       #00ad9c
*.color14:      #00ad9c

! white
*.color7:       #a8a19f
*.color15:      #f1efee

